<?xml version="1.0" encoding="UTF-8"?>
<cronentries>
  <cron>
    <url>/rest/v1/cron/notifications</url>
    <description>Periodically check if any notifications need to be sent</description>
    <schedule>every 5 minutes</schedule>
  </cron>
  <cron>
    <url>/rest/v1/cron/archiver</url>
    <description>Auto closes incidents</description>
    <schedule>every day 02:00</schedule>
    <timezone>America/New_York</timezone>
  </cron>
  <cron>
    <url>/rest/v1/cron/mergeNotifications</url>
    <description>Periodically merge memcache notifications into datastore notifications</description>
    <schedule>every 1 minutes</schedule>
  </cron>
</cronentries>